<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_logo_intro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AplicacionProfeQuiz"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Vista.ResenaActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.RegistroActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.DetailActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.MainActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.IntroActivity"
            android:exported="false" />
        <activity
            android:name=".Vista.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>